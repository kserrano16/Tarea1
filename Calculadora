num1 = float(input("Ingresa el primer numero: ")
op = (input("Ingresa la operacion: "))
num2 = float(input("Ingresa el segundo numero: "))

if op == "+":
  print(num1 + num2)
elif op == "-":
  print(num1 - num2)
elif op == "/":
  print(num1 / num2)
elif op == "*":
  print(num1 * num2)
else:
  print("Operacion Invalida")
